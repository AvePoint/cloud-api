{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "learntemplate"
      
    ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright © 2024 AvePoint® Inc. All Rights Reserved.<br>Generated with <strong>AvePoint, Inc.</strong></span>",
      "_enableSearch": true,
      "pdf": false
    }
  }
}